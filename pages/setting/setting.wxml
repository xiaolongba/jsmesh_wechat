<view class="page">
    <view class="page__bd page__bd_spacing">
        <view class="kind-list">
            <block wx:for="{{list}}" wx:key="id">
                <view class="kind-list__item">
                    <view id="{{item.id}}" class="weui-cell weui-cell_access" bindtap="kindToggle">
                        {{item.name}}
                    </view>

                </view>
            </block>
        </view>
    </view>

</view>